                    <text type="transcription" subtype="Takashima" xml:id="BB-001.text-1.text-transcription-Takashima">
                        <body>
                            <ab>
                                <seg xml:id="BB-001.text-1.text-transcription-Takashima.seg-1" n="1">壬子卜，</seg>
                                <seg xml:id="BB-001.text-1.text-transcription-Takashima.seg-2" n="2">爭貞，</seg>
                                <seg xml:id="BB-001.text-1.text-transcription-Takashima.seg-3" n="3">自今日我󰹨󽕦。</seg>
                            </ab>
                        </body>
                    </text>
                    <text type="translation" subtype="Takashima" xml:id="BB-001.text-1.text-translation-Takashima">
                        <body>
                            <ab>
                                <seg xml:id="BB-001.text-1.text-translation-Takashima.seg-1" n="1">Crack making on the renzi day [49], </seg>
                                <seg xml:id="BB-001.text-1.text-translation-Takashima.seg-2" n="2">Zheng tested : </seg>
                                <seg xml:id="BB-001.text-1.text-translation-Takashima.seg-3" n="3">Starting from the present, in five days (or: In five days
                                    from now), we (will) (be able to) harm (or: smite) the
                                    You.</seg>
                            </ab>
                        </body>
                    </text>
                </group>
            </text>
            <text xml:id="BB-001.text-2" n="2">
                <group>
                    <xi:include href="/db/tls-data/CHANT/b/uuid-bd568406-4f41-4487-a085-7344c8410e59.xml" xpointer="H-06834-正.text-2.text-transcription-yuanwen">
